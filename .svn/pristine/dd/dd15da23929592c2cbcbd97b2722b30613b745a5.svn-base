                    HorizontalAlignment="Left"
                    Orientation="Horizontal"
                    >
            <StackPanel x:Name="Temp_ZoneOne"
                        Margin="25,5,5,5"
                        VerticalAlignment="Top"
                        Background="LightGray"
                        Orientation="Vertical"
                        >

                <Label HorizontalAlignment="Center"
                       Content="ЗОНА 1"
                       FontSize="16"
                       Foreground="Black"
                       />

                <Border BorderThickness="2">
                    <Border.BorderBrush>
                        <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                            <GradientStop Offset="0" Color="Black" />
                            <GradientStop Offset="1" Color="White" />
                        </LinearGradientBrush>
                    </Border.BorderBrush>

                    <StackPanel x:Name="Sliders_ZoneOne"
                                Background="Gray"
                                Orientation="Horizontal"
                                >

                        <Slider Width="20"
                                Height="200"
                                Margin="15,10,0,10"
                                HorizontalAlignment="Left"
                                BorderBrush="White"
                                BorderThickness="1"
                                Maximum="300"
                                Minimum="0.0"
                                Orientation="Vertical"
                                Value="{Binding Path=Zone1TempReference}"
                                >
                            <Slider.Background>
                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                    <GradientStop Offset="0" Color="Red" />
                                    <GradientStop Offset="0.25" Color="Orange" />
                                </LinearGradientBrush>
                            </Slider.Background>
                        </Slider>
                        <Slider Width="20"
                                Height="200"
                                Margin="10,10,0,10"
                                HorizontalAlignment="Right"
                                BorderBrush="White"
                                BorderThickness="1"
                                Orientation="Vertical"
                                >
                            <Slider.Background>
                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                                    <GradientStop Color="Red" />
                                    <GradientStop Offset="0.25" Color="SteelBlue" />
                                </LinearGradientBrush>
                            </Slider.Background>
                        </Slider>
                    </StackPanel>
                </Border>
                <Border BorderBrush="SteelBlue" BorderThickness="1">
                    <StackPanel Background="White" Orientation="Horizontal">
                        <Rectangle Width="31"
                                   Height="31"
                                   Margin="5"
                                   Fill="{StaticResource lHeatOn}"
                                   />
                        <Rectangle Width="31"
                                   Height="31"
                                   Margin="5"
                                   Fill="{StaticResource lCoolOn}"
                                   />

                    </StackPanel>
                </Border>

